{
  "daemon_name": "NSF_default",

  "daemon_display_name": "NOOXY service",

  "description": "A NOOXY Service Framework Daemon.",

  "connection_servers": [
    {
      "type": "WebSocketSecure",
      "ip": "0.0.0.0",
      "port": 1877
    },
    {
      "type": "WebSocket",
      "ip": "0.0.0.0",
      "port": 1487
    },
    {
      "type": "TCP/IP",
      "ip": "0.0.0.0",
      "port": 2212
    }
  ],

  "default_server": 0,

  "local_services_owner" : "admin",

  "services_path": "services/",

  "services_files_path": "service_files/",

  "database_path": "db.sqlite3",

  "SHA256_key": "FATFROG",

  "services": [
    "NoServiceManager",
    "NoNoti",
    "NoUser",
    "SimpleNoti",
    "Noversi",
    "NoActivity"
  ],

  "trusted_domains": [
    "www.nooxy.tk",
    "127.0.0.1",
    "0.0.0.0",
    "localhost"
  ],

  "shell_service": "NoShell",

  "shell_client_service": "NoShellc",

  "debug_service": "NoDebug",

  "debug": true,

  "debug_config": {
  	"show_json": true,
	"show_conn_data": false
  },

  "verbose": true,

  "shell_client_service_delay": 0,

  "only_trust_origin_domain": false,

  "operations_timeout": 320,

  "token_expire_period": 7,

  "daemon_authorization_key": "WHATAKEY",

  "secure": false,

  "rsa_2048_pub_key": "public_key.pem",

  "rsa_2048_priv_key": "private_key.pem",

  "ssl_priv_key": "ssl.key",

  "ssl_cert": "ssl.crt",

  "kill_daemon_timeout": 3000,

  "heartbeat_cycle": 60000

}
